generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Conexiones {
  id  Int @unique @default(autoincrement())
  personas Int
  temp Int
}

model Estaciones {
  id Int @unique @default(autoincrement())
  estacion String 
}

model Lineas {
  id Int @unique @default(autoincrement())
  linea String
}
